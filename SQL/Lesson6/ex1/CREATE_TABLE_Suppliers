CREATE TABLE Suppliers(
--CONSTRAINT CONST-NAME CONSTRAINT TYPE (COLUMN NAME)
SupplierID INT,
CONSTRAINT Suppliers_id PRIMARY KEY (SupplierID),
CompanyName NVARCHAR2(20),
ContactName NVARCHAR2(20),
ContactTitle NVARCHAR2(50),
Adress NVARCHAR2(30),
City NVARCHAR2(20),
Region NVARCHAR2(20),
PostalCode NUMBER(20),
Country NVARCHAR2(20),
Phone NUMBER(20),
Fax NUMBER(20),
HomePage NVARCHAR2(50)
);